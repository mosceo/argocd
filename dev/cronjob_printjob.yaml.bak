apiVersion: batch/v1
kind: CronJob
metadata:
  name: printjob
spec:
  schedule: "* * * * *"
  jobTemplate:
    spec:
      template:
        spec:
          containers:
          - name: printjob
            image: mosceo/printjob:1.0
            imagePullPolicy: IfNotPresent
          restartPolicy: OnFailure